<h2>Trello mockup</h2>
{{#each testCollection}}
<div class="list" data-listid="{{listId}}">
	<h3>List</h3>
	<ul>
		<li>LIST Heading: {{heading}}</li>
		<li>LIST subscribed: {{subscribed}}</li>
	</ul>
	{{#each cards}}	
	<div class="card" data-cardid="{{cardId}}">
		<h3>Card</h3>
		<ul>
			<li>CARD Label: {{label}}</li>
			<li>CARD Description: {{description}}</li>
			<li>CARD Due: {{due}}</li>
		</ul>
		<h3>Comments</h3>
			{{#each comments}}
			<div class="comments">
				<ul>
					<li>{{this}}</li>
				</ul>
			</div>
			{{/each}}
	</div>
	{{/each}}	
</div>
{{/each}}
